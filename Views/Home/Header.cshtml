@model Website_TecSys_NetCore.Models.ConfiguracoesModel;
<header class="header">
    <div class="d-flex justify-content-between align-items-center up-header">
        <div class="">
            <span><i class="fa fa-phone-alt"></i> @Model.Telefone</span>
            <span><i class="fa fa-envelope"></i> @Model.Email</span>
        </div>
        <div>
            <button class="btn btn-primary btn-rounded">...</button>
        </div>
    </div>
    <div class="d-flex justify-content-between align-items-center bottom-header">
        <img src="~/images/logo.png" title="imagem" class="logo"/>
        <ul class="menu mb-0 d-flex align-items-center">
            @{foreach(var menu in ViewBag.Menus){
                <li class="menu-item">
                    <a href="#">@menu</a>
                </li>
            }}
            
        </ul>
    </div>
</header>